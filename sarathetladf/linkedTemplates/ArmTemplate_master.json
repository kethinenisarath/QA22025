{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sarathetladf"
		},
		"LS_BOB_SOURCE_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BOB_SOURCE'"
		},
		"LS_GEN2_SINK_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_GEN2_SINK'"
		},
		"LS_SINK_GOBAL_PARAMATER_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_SINK_GOBAL_PARAMATER'"
		},
		"LS_SOURCE_GOBAL_PARAMETERS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_SOURCE_GOBAL_PARAMETERS'"
		},
		"LS_GEN2_SINK_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dsgen2sink.dfs.core.windows.net/"
		},
		"LS_SINK_GOBAL_PARAMATER_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://etlsoure.dfs.core.windows.net/"
		},
		"LS_SOURCE_GOBAL_PARAMETERS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://etlsoure.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sarathetladf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_BOB_SOURCE_connectionString": {
						"value": "[parameters('LS_BOB_SOURCE_connectionString')]"
					},
					"LS_GEN2_SINK_accountKey": {
						"value": "[parameters('LS_GEN2_SINK_accountKey')]"
					},
					"LS_SINK_GOBAL_PARAMATER_accountKey": {
						"value": "[parameters('LS_SINK_GOBAL_PARAMATER_accountKey')]"
					},
					"LS_SOURCE_GOBAL_PARAMETERS_accountKey": {
						"value": "[parameters('LS_SOURCE_GOBAL_PARAMETERS_accountKey')]"
					},
					"LS_GEN2_SINK_properties_typeProperties_url": {
						"value": "[parameters('LS_GEN2_SINK_properties_typeProperties_url')]"
					},
					"LS_SINK_GOBAL_PARAMATER_properties_typeProperties_url": {
						"value": "[parameters('LS_SINK_GOBAL_PARAMATER_properties_typeProperties_url')]"
					},
					"LS_SOURCE_GOBAL_PARAMETERS_properties_typeProperties_url": {
						"value": "[parameters('LS_SOURCE_GOBAL_PARAMETERS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}